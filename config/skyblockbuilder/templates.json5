{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south",
  //     "offset": [ 0, 0 ],
  //     "offsetY": 0,
  //     "surroundingMargin": 0,
  //     "surroundingBlocks": "default"
  // }
  "spawn": null,
  
  "spawns": {
    "default": [
      [ 8, 4, 8]
    ]
  },
  
  // A list of blocks which can be used to surround islands/caves.
  "surroundingBlocks": {
    "default": []
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "Compact Sky",
      "file": "compact_sky.nbt",
      "spawns": "default",
      "direction": "south",
      "surroundingBlocks": "default"
    }
  ]
}
